/* pages/home/home.wxss */
.home {
  background-color: #eaf1f7;
}
.imgBox {
  width: 100%;
  height: 160px;
}
.temItem {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 90%;
  height: 100px;
  margin-left: 5%;
  margin-top: 10px;
  border-radius: 5px;
}
.warn_tem {
  height: 100%;
  width: 30%;
  background-color: #ffa149;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.nor_tem {
  height: 100%;
  width: 30%;
  background-color: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.itemRight {
  width: 70%;
  height: 100%;
  background-color: #fff;
}
.temp_type {
  font-size: 12px;
  color: #fff;
}
.nor_temp_type {
  font-size: 12px;
}
.temp {
  font-size: 24px;
  color: #fff;
  margin-top: 10px;
}
.nor_temp {
  font-size: 24px;
  margin-top: 10px;
  color: #2178ff;
}
.temp_title {
  border-bottom: 1px solid #f6f6f6;
  line-height: 40px;
  font-size: 14px;
  margin-left: 10px;
}
.start {
  position: relative;
  left: 50px;
  font-size: 18px;
}
.temp_con {
  font-size: 14px;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  height: 60px;
}
.con_item {
  color: #868686;
}
.tabbars {
  margin-top: 50px;
}